<table class="table table-condensed">
	<thead>
		<tr>
			<th> id </th>
			<th> Name </th>
			<th> Application ID </th>
			<th> Private Key </th>
		<tr>
	</thead>
	<tbody>
		<% @projects.each do |project| %>
			<tr>
				<td> <%= project.id %> </td>
				<td> <%= project.name %> </td>
				<td> <%= link_to "#{project.application_id}", project_queues_url(project) %> </td>
				<td> <%= project.private_key %> </td>
			</tr>
		<% end %>
	</tbody> 
</table>


<%= link_to "New Project", new_user_project_url(current_user) %>